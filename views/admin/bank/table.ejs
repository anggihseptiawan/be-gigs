<table id="example2" class="table table-bordered table-hover">
	<thead>
		<tr>
			<th>No.</th>
			<th>Nama Pemilik</th>
			<th>Nama Bank</th>
			<th>Nomor Rekening</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% for( let i = 0; i < bank.length; i++ ) { %>
		<tr>
			<td><%= i + 1 %></td>
			<td><%= bank[i].name %></td>
			<td><%= bank[i].bankName %></td>
			<td><%= bank[i].rekeningNumber %></td>
			<td>
				<form
					action="bank/delete/<%= bank[i]._id %>?_method=DELETE"
					method="POST"
				>
					<a href="bank/edit/<%= bank[i]._id %> " class="btn btn-warning"
						>Ubah</a
					>
					<button class="btn btn-danger">Hapus</button>
				</form>
			</td>
		</tr>
		<% } %>
	</tbody>
</table>
